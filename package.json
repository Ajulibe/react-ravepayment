{
  "name": "react-ravepayment",
  "version": "3.0.0",
  "description": "This is a reactJS library for implementing rave payment gateway",
  "scripts": {
    "test": "tsc --noEmit && mocha --reporter spec test/testSetup.js \"test/**/*.spec.js\"",
    "format": "prettier --write '**/**/*.{js,}'",
    "build": "rm -rf dist && tsc",
    "lint": "eslint --ext .js,.ts,.tsx,.jsx --ignore-path .gitignore .",
    "lint:ci": "eslint --ext .js,.ts,.tsx,.jsx --ignore-path .gitignore --report-unused-disable-directives .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamraphson/react-ravepayment.git"
  },
  "keywords": [
    "Javascript",
    "github",
    "ReactJS",
    "Open Source",
    "payments",
    "rave",
    "ravepayment",
    "flutterwave",
    "Gateway"
  ],
  "author": "Ayeni Olusegun <nsegun5@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamraphson/react-ravepayment/issues"
  },
  "homepage": "https://github.com/iamraphson/react-ravepayment#readme",
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.15.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "^2.1.1",
    "sinon": "^7.1.1",
    "typescript": "^3.6.3"
  }
}
